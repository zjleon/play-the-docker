{
  "name": "web_static",
  "version": "1.0.0",
  "description": "static web service",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run dev:web_static",
    "start_prod": "better-npm-run prod:web_static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "dev:web_static": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 8080,
        "APIPORT": 8081
      }
    },
    "dev:dirTree": {
      "command": "node ./dirTree.dev.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development"
      }
    },
    "prod:web_static": {
      "command": "node ./webpack.prod.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 8081
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-npm-run": "0.0.14",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "express": "^4.14.1"
  },
