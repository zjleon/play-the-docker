    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "material-ui": "^1.0.0-beta.45",
    "moment": "^2.19.1",
    "npm": "^6.0.1",
    "prop-types": "^15.5.10",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sharp": "^0.20.2",
    "stylus": "^0.54.5",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-merge": "^4.1.2",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
