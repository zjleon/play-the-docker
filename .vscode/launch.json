  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "seed poker server",
      "cwd": "${workspaceFolder}/services/seed_poker_server",
      "program": "${workspaceFolder}/services/seed_poker_server/index.js",
      "env": {
        "NODE_ENV":"development",
        "PORT": "3000",
        "MAX_PLAYER": "7"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
  ]
}