  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "seed poker server",
      "cwd": "${workspaceFolder}/services/seed_poker_server",
      "program": "${workspaceFolder}/services/seed_poker_server/node_modules/gulp/bin/gulp.js",
      "args": [
        "--gulpfile",
        "configs/gulp/index.js",
      ],
      "autoAttachChildProcesses": true,
      "env": {"NODE_ENV": "development"}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "seed poker AI",
      "cwd": "${workspaceFolder}/services/seed_poker_AI",
      "program": "${workspaceFolder}/services/seed_poker_AI/node_modules/gulp/bin/gulp.js",
      "args": [
        "--gulpfile",
        "configs/gulp/index.js",
      ],
      "autoAttachChildProcesses": true,
      "env": {"NODE_ENV": "development"}
    },
